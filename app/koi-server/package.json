{
  "name": "koi-server",
  "main": "./src/main.ts",
  "scripts": {
    "build": "nest build --config nest-cli.production.json",
    "build:analyzer": "cross-env IS_ANALYZER=true nest build --config nest-cli.production.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest build --config nest-cli.development.json --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "turbo:dev": "turbo run dev"
  },
  "dependencies": {
    "@codegenie/serverless-express": "^4.13.0",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/mongoose": "^10.0.2",
    "@nestjs/platform-express": "^10.3.0",
    "@toss/date": "^1.1.7",
    "@toss/utils": "^1.4.4",
    "aws-lambda": "^1.0.7",
    "class-transformer": "^0.5.1",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "feature-nest-poll": "workspace:*",
    "mongoose": "^8.1.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.3.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.5.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.7",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "29.5.0",
    "prettier": "^2.6.1",
    "run-script-webpack-plugin": "^0.2.0",
    "shared~type": "workspace:*",
    "shared~type-koi": "workspace:*",
    "shared~type-party": "workspace:*",
    "shared~type-poll": "workspace:*",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "turbo": "^1.11.2",
    "typescript": "^5.1.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "webpack-pnp-externals": "^1.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
